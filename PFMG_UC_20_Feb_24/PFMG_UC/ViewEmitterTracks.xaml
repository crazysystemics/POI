<Window x:Class="PFMG_UC.ViewEmitterTracks"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PFMG_UC"
        mc:Ignorable="d"
        Title="ViewEmitterTracks" Height="450" Width="800">
    <Grid Background="#FFEFF9F9">
        <DataGrid x:Name="emitter_track_table" ItemsSource="{Binding}" Margin="31,68,61,68" CanUserAddRows="False" Background="#FFF8F6F6" IsReadOnly="True"/>
        <Button x:Name="btn_view_emitter_tracks" Content="View Emitter Tracks" HorizontalAlignment="Left" Margin="31,23,0,0" VerticalAlignment="Top" Width="118" Background="#FFF9D9AA" BorderBrush="#FFFFFEFE" Click="btn_view_emitter_tracks_Click"/>
        <ComboBox x:Name="cmb_emitter_tracks" HorizontalAlignment="Left" Margin="189,23,0,0" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
